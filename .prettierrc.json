{
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "plugins": ["prettier-plugin-organize-imports"],
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.js",
      "options": {
        "parser": "babel"
      }
    }
  ]
}
